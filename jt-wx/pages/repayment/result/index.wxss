/* pages/account/result/index.wxss */
.account-result{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.account-result image{
  width: 50px;
  height: 50px;
  display: block;
  margin: 100rpx 0 40rpx;
}
.account-result .title{
  font-size:16px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.account-result .info{
  font-size: 15px;
  color: #999999;
  margin-top: 5px;
}
.account-result button::after{
  display: none;
}
.account-result button{
  width: 190px;
  height: 50px;
  line-height: 50px;
  border-radius: 25px;
  border:1px solid rgba(255,44,67,1);
  font-size:17px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,44,67,1);
  background-color: transparent;
  margin-top: 140rpx;
}
.account-result button.fill{
  background:rgba(255,44,67,1);
  box-shadow:0px 2px 8px 0px rgba(255,44,67,0.32);
  color: white;
}
.account-result button.error{
  margin-top: 21px;
}