/* pages/withdraw/face/index.wxss */
.account-face{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.account-face .title {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 47px 0 15px;
  font-size: 17px;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
}
.account-face image{
  width: 132px;
  height: 94px;
  display: block;
  margin: 146rpx 0;
}
.account-face>text{
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
}
.account-face button::after{
  display: none;
}
.account-face button{
  margin-top: 16px;
  width: 250px;
  height: 50px;
  line-height: 50px;
  background:rgba(255,44,67,1);
  box-shadow:0px 2px 8px 0px rgba(255,44,67,0.32);
  border-radius:25px;
  font-size:17px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.account-face .button-hover{
  background:rgba(255,44,67,1);
}