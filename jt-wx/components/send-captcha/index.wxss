/* components/resend/index.wxss */
.send-captcha::after{
  display: none;
}
.send-captcha {
  display: inline-block;
  line-height: 1em;
  font-size: 13px;
  color: #0081FF;
  background-color: transparent;
  padding: 0;
}
.send-captcha.icon{
  padding: 0 0 0 20px;
  background: transparent url("data:image/png;base64,") no-repeat 0 50%;
  background-size: 15px 15px;
}
button.send-captcha[disabled]:not([type]){
  background-color: transparent;
}
.send-captcha[disabled]{
  background-image: none;
  color: #BFBFBF;
}