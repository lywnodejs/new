/* components/handWrite/index.wxss */

.canvasBox{
  width: 100%;
  height: 100%;
  overflow: hidden;
  border: 1px solid #e8e8e8;
  box-sizing: border-box;
  background-color: #f6f6f6;
  position: relative;
}
canvas{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}
.canvasBox>.bg{
  z-index: 1;
}