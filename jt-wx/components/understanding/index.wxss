/* components/understanding/index.wxss */
.understanding {
  display: flex;
  flex-direction: row;
  flex-flow: wrap;
  background-color: white;
  margin-top: 12px;
  border-radius: 5px;
  padding: 30rpx 20rpx;
}
.understanding .cell{
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  width: 210rpx;
  height: 192rpx;
}
.understanding .cell image {
  width: 50px;
  height: 50px;
}
.understanding .cell text {
  color: #666666;
  font-size: 13px;
  margin-top: 8px;
}