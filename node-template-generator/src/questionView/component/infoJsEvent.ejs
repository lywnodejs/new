    function clickPersons(this_) {//人物点击
        var val = this_.attr('data-txt');
        $('.itemBottomBox .topItem').eq(2).find('span').text('');
        if(person_active.length===0){
            $('.itemBottomBox .topItem').eq(2).find('span').text('');
        }
        if (person_active.indexOf(val) == -1) {
            // this_.addClass('rxh_on');
            person_active.push(val);
            $('#renWuAlert .rxh_scrollBox').eq(1).append($('#renWuAlert .rxh_scrollBox').eq(0).find('.' + val));
            $('#renWuAlert .rxh_scrollBox').eq(0).find('.' + val).hide(0);
            $('#personsActive').append('<b class="' + val + '' + 2 + '" data-txt="' + val + '" onclick="clickPersons($(this))"><span>人物：</span><em>' + val + '</em><a><i class="icon-close_circle rxh_icon_nationwide"></i></a></b>')
        } else {
            person_active.splice(person_active.indexOf(val), 1);
            // $('.' + val + "2").remove();
            // $('.' + val + "2").removeClass('active_li');
            // $('.' + val).removeClass('rxh_on');
            // $('#renWuAlert .rxh_scrollBox').eq(0).find('.' + val).show(0);
            // $('#renWuAlert .rxh_scrollBox').eq(1).find('.' + val).remove();
        }
        // if (person_active.length === 0 && text_active.length === 0 && police_active.length === 0 && policy_active.length === 0 && industry_active.length === 0) {
        //     $('#personsActive').hide(0);
        // } else {
        //     $('#personsActive').show(0);
        // }
        if(person_active.length===0){
            $('.itemBottomBox .topItem').eq(2).find('span').text('人物');
            $('.itemBottomBox .topItem').eq(2).find('span').css({color:'#606980'});
        }else{
            $('.itemBottomBox .topItem').eq(2).find('span').css({color:'#5C8DF7'});
            $('.itemBottomBox .topItem').eq(2).find('span').text(person_active.join(','));
        }
        PersonsParams.person = person_active.join(',');
        methods.getClass();
        var text = $('.top p').eq(0).find('span').text();
        listParams.cp = 1;
        listParams.priorities = '';
        if (text !== '按综合排序') {
            listParams.person = person_active.join(',');
            $('.listItem .ul').html('');
            methods.getInfinity('#list_xxx', listParams, true);
        } else {
            listParams.person = person_active.join(',');
            $('.listItem .ul').html('');
            methods.getData(listParams);
        }


    }

    function clickPolicy(this_) {//政策标签点击
        var val = this_.attr('data-txt');
        eleAll();
        if (policy_active.indexOf(val) == -1) {
            // this_.addClass('rxh_on');
            policy_active.push(val);
            $('#ZCAlert .rxh_scrollBox').eq(1).append($('#ZCAlert .rxh_scrollBox').eq(0).find('.' + val));
            $('#ZCAlert .rxh_scrollBox').eq(0).find('.' + val).hide(0);
            $('#personsActive').append('<b class="' + val + '' + 2 + '" data-txt="' + val + '" onclick="clickPolicy($(this))"><span>政策标签：</span><em>' + val + '</em><a><i class="icon-close_circle rxh_icon_nationwide"></i></a></b>')
        } else {
            policy_active.splice(policy_active.indexOf(val), 1);
            $('.' + val + "2").remove();
            $('.' + val + "2").removeClass('rxh_on');
            $('.' + val).removeClass('rxh_on');
            $('#ZCAlert .rxh_scrollBox').eq(0).find('.' + val).show(0);
            $('#ZCAlert .rxh_scrollBox').eq(1).find('.' + val).remove();
        }
        if (person_active.length === 0 && text_active.length === 0 && police_active.length === 0 && policy_active.length === 0 && industry_active.length === 0) {
            $('#personsActive').hide(0);
        } else {
            $('#personsActive').show(0);
        }
        PersonsParams.policyThemesN = policy_active.join(',');
        methods.getClass();
        if (type === '会议' || type === '考察') {
            listParams.policyThemesN = policy_active.join(',');
            methods.getMeeting(type, 1, true);
            return;
        }
        var text = $('#rxh_g_txt').text();
        listParams.cp = 1;
        listParams.priorities = '';
        if (text !== '按综合') {
            listParams.policyThemesN = policy_active.join(',');
            $('.rxh_nat_news ul').html('');
            methods.getInfinity('#list_xxx', listParams, true);
        } else {
            listParams.policyThemesN = policy_active.join(',');
            $('.rxh_nat_news ul').html('');
            methods.getData(listParams);
        }


    }

