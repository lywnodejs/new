    function clickPolice(this_) {//点击机构
        var val = this_.attr('data-txt');
        var test = val.split('|')[val.split('|').length - 1];
        if(police_active.length===0){
            $('.itemBottomBox .topItem').eq(1).find('span').text('');
        }
        eleAll();
        if (police_active.indexOf(val) == -1) {
            $('.itemBottomBox .topItem').eq(1).find('span').css({color:'#5C8DF7'});
            $('.itemBottomBox .topItem').eq(1).find('span')[0].innerHTML+=test+',';
