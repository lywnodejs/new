    /**
     * @description: 点击时间
     * @param ele
     * @param val
     */
    function getDate_shijian(val,index) {
        var end = new Date().getTime();
        var begin7 = end - 3600 * 1000 * 24 * 7;
        var begin30 = end - 3600 * 1000 * 24 * 30;
        var begin360 = end - 3600 * 1000 * 24 * 360;
        if (val === '最近七天') {
            startDate_sele = begin7;
        } else if (val === '最近一个月') {
            startDate_sele = begin30;
        } else if (val === '最近一年') {
            startDate_sele = begin360;
        } else if (val === '十九大以来') {
            startDate_sele = 1508256000000;
        }
        eleAll();
        listParams.priorities = '';
        listParams.begin = startDate_sele;
        if(val!==''){
            $('.selectionDate p').eq(0).text('选择开始时间');
            $('.selectionDate p').eq(1).text('选择结束时间');
            listParams.end = end;
        }
        listParams.cp = 1;
        $('.listItem .ul').html('');
        var text = $('.top p').eq(0).find('span').text();
        if (text !== '按综合排序') {
            methods.getInfinity('#list_xxx', listParams, true);
        } else {
            methods.getData(listParams);
        }
        $('.rw').hide();
        $('.sj').hide();
        $('.jg').hide();
        $('.tc').hide();
        $('.module_Box').hide();
        $('.sjItem').find('li').removeClass('active_li');
        $('.sjItem').find('li').eq(index).addClass('active_li');
    }
