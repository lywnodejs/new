    function clickText(this_) {//点击体裁
        var val = this_.attr('data-txt');
        var test = val.split('|')[val.split('|').length - 1];
        eleAll();
        if(text_active.length==0){
            $('.itemBottomBox .topItem').eq(0).find('span').text('');
        }
        if (text_active.indexOf(val) == -1) {
            $('.itemBottomBox .topItem').eq(0).find('span')[0].innerHTML+=test;
            $('.itemBottomBox .topItem').eq(0).find('span').css({color:'#5C8DF7'});
            // $('#TCAlert .rxh_scrollBox').eq(1).append($('#TCAlert .rxh_scrollBox').eq(0).find('.' + test));
            // $('#TCAlert .rxh_scrollBox').eq(0).find('.' + test).hide(0);
