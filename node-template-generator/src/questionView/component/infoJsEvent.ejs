    /**
     * @description 点击综合
     * @param val[string]:条件
     **/
    function zhClick(val, index) {
        var text = val;
        eleAll();
        $('.zh li').removeClass('active_li');
        $('.zh li').eq(index).addClass('active_li');
        listParams.priorities = '';
        if (text === '按综合排序') {
            $('#zhpx').show();
            $('#zhpx .listItem').hide();
            $('#zhpxH').hide();
            listParams.cp = 1;
            listParams.orderBy = '';
            listParams.direction = '';
            $('.top p').eq(0).find('span').text('按综合排序');
            methods.getData(listParams);
        } else if (text === '按相关度排序') {
            listParams.cp = 1;
            listParams.orderBy = '';
            listParams.direction = '';
            $('#zhpx').hide();
            $('#zhpxH').show();
            $('.top p').eq(0).find('span').text('相关度排序');
            methods.getInfinity('#list_xxx', listParams, true);
        } else if (text === '按发布时间从新到旧') {
            listParams.cp = 1;
            listParams.orderBy = 'publishAt';
            listParams.direction = 'desc';
            $('.top p').eq(0).find('span').text('发布时间从新到旧');
            $('#zhpx').hide();
            $('#zhpxH').show();
            methods.getInfinity('#list_xxx', listParams, true);
        } else if (text === '按发布时间从旧到新') {
            listParams.cp = 1;
            listParams.orderBy = 'publishAt';
            listParams.direction = 'asc';
            $('.top p').eq(0).find('span').text('发布时间从旧到新');
            $('#zhpx').hide();
            $('#zhpxH').show();
            methods.getInfinity('#list_xxx', listParams, true);
        }
        $('.listItem .ul').html('');
        $('.module_Box').click();
    }

    /**
     * @description 点击全部
     * @param val[string]:条件
     **/
    function searchClassClick(val,index) {
        var input_class = val;
        $('.nr li').removeClass('active_li');
        $('.nr li').eq(index).addClass('active_li');
        if (input_class === '搜全部') {
            $('.top p').eq(1).find('span').text('搜全部');
            listParams.title = input_search_val;
            listParams.content = input_search_val;
            listParams.issuedNumber = input_search_val;
            listParams.attachmentNames = input_search_val;
            listParams.plainHighlightedFieldName = 'title,content,attachmentNames';
        } else if (input_class === '搜标题') {
            $('.top p').eq(1).find('span').text('搜标题');
            listParams.title = input_search_val;
            listParams.plainHighlightedFieldName = 'title';
            listParams.title = input_search_val;
            listParams.content = '';
            listParams.issuedNumber = '';
            listParams.attachmentNames = '';
        } else if (input_class === '搜正文') {
            $('.top p').eq(1).find('span').text('搜正文');
            listParams.content = input_search_val;
            listParams.title = '';
            listParams.issuedNumber = '';
            listParams.attachmentNames = '';
            listParams.plainHighlightedFieldName = 'content';
        } else if (input_class === '搜文号') {
            $('.top p').eq(1).find('span').text('搜文号');
            listParams.issuedNumber = input_search_val;
            listParams.content = '';
            listParams.title = '';
            listParams.attachmentNames = '';
            // data.plainHighlightedFieldName='issuedNumber';
        } else if (input_class === '搜附件') {
            $('.top p').eq(1).find('span').text('搜附件');
            listParams.attachmentNames = input_search_val;
            listParams.plainHighlightedFieldName = 'attachmentNames';
        }
        eleAll();
        listParams.cp=1;
        listParams.priorities = '';
        var text = $('.top p').eq(0).find('span').text();
        listParams.cp = 1;
        if (text !== '按综合排序') {
            $('.listItem .ul').html('');
            methods.getInfinity('#list_xxx', listParams, true);
        } else {
            $('.listItem .ul').html('');
            methods.getData(listParams);
        }
        $('.module_Box').click();
    }

