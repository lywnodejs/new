            $('.' + val.split('|')[val.split('|').length - 1] + '2').remove();
            $('.' + val.split('|')[val.split('|').length - 1]).removeClass('active_li');
        }
        if(text_active.length==0){
            $('.itemBottomBox .topItem').eq(0).find('span').text('体裁');
            $('.itemBottomBox .topItem').eq(0).find('span').css({color:'#606980'})
