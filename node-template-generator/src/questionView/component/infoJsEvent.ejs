            $('#personsActive').append('<b class="' + val.split('|')[val.split('|').length - 1] + '2" data-txt="' + val + '" onclick="clickPolice($(this))"><span>机构：</span><em>' + val.split('|')[val.split('|').length - 1] + '</em><a><i class="icon-close_circle rxh_icon_nationwide"></i></a></b>')
