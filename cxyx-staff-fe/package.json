    "@didi/webpack-gift-oss-plugin": "^0.0.4",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.41",
    "antd-mobile": "^2.3.4",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "qrcode.react": "^1.0.1",
    "rc-form": "^2.4.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.3.0",
    "umi-request": "^1.3.5",
    "uuid": "^8.3.2",
    "xlsx": "^0.16.9",
    "yorkie": "^2.0.0"
