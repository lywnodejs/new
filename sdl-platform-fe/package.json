  "private": true,
  "scripts": {
    "start": "baymax-scripts start",
    "style": "lessc style/index.less static/style.css",
    "theme": "et -i ./style/element/element-variables.scss",
    "theme:blue": "et -i ./style/element/element-variables-blue.scss",
    "theme:orange": "et -i ./style/element/element-variables-orange.scss",
    "build": "baymax-scripts build",
    "build:theme": "et -c ./style/element/element-variables.scss -o ./static/element/default",
    "build:theme:blue": "et -c ./style/element/element-variables-blue.scss -o ./static/element/blue",
    "build:theme:orange": "et -c ./style/element/element-variables-orange.scss -o ./static/element/orange",
    "dll": "baymax-scripts dll",
    "lint": "eslint --ext .js,.vue src",
    "deploy": "baymax-scripts deploy"
  },
  "dependencies": {
    "@didi/omega-tracker": "^2.2.9",
    "ajv": "5.2.2",
