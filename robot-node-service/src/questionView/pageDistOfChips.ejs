    <link rel="stylesheet" href="static/css/cmfb_style.css">
    <script src="static/js/libs/rem.js"></script>
    <style>
        *{ margin: 0; padding: 0;}

        /********************************************************************************/

        .box_ChipDis{ font-size: 12px; line-height: 20px; width: 223px; height: 210px; position: relative;}
        .box_ChipDis .cloPrice,
        .box_ChipDis .higPrice,
        .box_ChipDis .lowPrice,
        .box_ChipDis ul{ position: absolute; z-index: 1;}
        .box_ChipDis .cloPrice{ border-bottom: dashed 1px #a1a2a8; color: #333; width: 130px; height: 20px; text-align: right; padding-right: 10px; left: 44px; z-index: 2;}
        .box_ChipDis .higPrice,
        .box_ChipDis .lowPrice{ color: #a1a2a8; height: 20px; width: 40px; text-align: right; left: 0;}
        .box_ChipDis .higPrice{ top: -10px;}
        .box_ChipDis .lowPrice{ bottom: -10px;}
        .box_ChipDis ul{ border-left: solid 1px #ccd6eb; clear:both; list-style-type:none; zoom:1; width: 150px; height: 100%; left: 43px; top: 0;}
        .box_ChipDis li{ background: #639df5; height: calc(210px / 120); margin-top: calc(210px / 120);}
        .box_ChipDis li.b_red{ background: #e55656;}
        .box_ChipDis li{ position: relative;}
        .box_ChipDis li .cloPrice{ top: -22px; left: 0;}
        /********************************************************************************/

    </style>
</head>
