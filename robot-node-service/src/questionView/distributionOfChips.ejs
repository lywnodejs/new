<% include ./component/params %>
<% include ./component/preAnswer %>
<% include ./component/jsLoad %>
<div class='bd' id="answer<%= sn %>">
    <div class='mb'>
        <h6 class="date" id="lastShowDate<%= mathRandom %>">最后日期 <%= timeUtil.getTimeStr(t) %></h6>
        <div class="box_show">
            <ul class="cmfb_shares">
                <li class="cmfb_name" id="titleCYQ<%= mathRandom %>"></li>
            </ul>
            <div class="cmfb_cyq">
                <div>
                    <div class="box_ChipDis" id="box_ChipDis<%= mathRandom %>" style="width: 220px; height: 210px;">
                        <div class="higPrice" id="higPrice<%= mathRandom %>">0.0</div>
                        <div class="lowPrice" id="lowPrice<%= mathRandom %>">0.0</div>
                        <ul id="ul_li<%= mathRandom %>"></ul>
                    </div>
                </div>
                <i id="fs<%= mathRandom %>" class="icon-full"></i>
            </div>
        </div>
    </div>
</div>
<script>

  try {
    thas.nodeFunction('questionAnalyse', '<%- JSON.stringify(info.questionAnalyse[0]) %>');
  } catch (error) {

  }

  var jsNo = 0;
  var url = '<%= config %>';
  var mathRandom = '<%= mathRandom %>'
  var frameId = '<%= frameId %>';
  var symbol = '<%= symbol %>';
  var code = '<%= code %>';
  var name = '<%= name %>';
  var sn = '<%= sn %>'

  var distributionOfChips = {
    onViewReady: function () {
      newKlineCYQ.init(symbol, code, name, 'v', mathRandom, false, url);
      resizeUtils.init(frameId, sn);
    }
  }
  var jsArr = ['/static/js/libs/jquery-1.11.2.min', '/static/js/utils/logUtil', '/static/js/utils/timeUtil', '/static/js/charts/jKlineCYQInit', '/static/js/charts/jKlineCYQ', '/static/js/utils/resizeUtils','/static/js/utils/toolsUtil'];
  loadJs(jsArr, distributionOfChips.onViewReady)

</script>





